! Xft
Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hinstyle: hintslight
!Xft.hinstyle: hintsfull
Xft.autohint:0
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
Xft.antialias: 1

! URxvt
URxvt.scrollBar: false
URxvt.loginShell: true
URxvt.font: xft:Monaco for Powerline:pixelsize=16
URxvt.termName: rxvt-256color
URxvt.underlineURLs: true
URxvt.urlLauncher: google-chrome
URxvt.perl-ext-common:  default,matcher
URxvt.matcher.button:   1 
URxvt.perl-lib: /home/akira/.urxvt/
URxvt.perl-ext: selection,mark-yank-urls,font-size
URxvt.keysym.C-equal: perl:font:increase
URxvt.keysym.C-minus: perl:font:decrease

