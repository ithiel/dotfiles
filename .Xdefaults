! Xft
Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hinstyle: hintslight
Xft.autohint:0
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.lcdfilter: lcddefault
Xft.antialias: 1

! URxvt
!URxvt*transparent: true
!URxvt*shading: 15
URxvt.scrollBar: false
URxvt.loginShell: true
!*font: -*-lucidatypewriter-medium-*-*-*-12-*-*-*-*-*-*-*
URxvt.font: xft:monaco:size=13
XTERM.font: xft:monaco:size=13
!URxvt.boldFont: xft:envy code r:medium:pixelsize=13
URxvt.termName: rxvt-256color
!URxvt.background: #1a1a1a
!URxvt.foreground: #e2e2e5
URxvt.underlineURLs: true
!URxvt.urlLauncher:      /usr/bin/firefox
URxvt.urlLauncher: google-chrome
URxvt.perl-ext-common:  default,matcher
URxvt.matcher.button:   1 
URxvt.perl-lib: /home/username/.urxvt/
URxvt.perl-ext: selection,mark-yank-urls,font-size
URxvt.keysym.C-equal: perl:font:increase
URxvt.keysym.C-minus: perl:font:decrease
